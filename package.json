{
  "name": "vuelendar-lp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "vue-cli-service serve",
    "dev:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --mode=development",
    "build": "vue-cli-service build --modern",
    "build:ci": "vue-cli-service build --modern --report",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "eslint --fix --ext .js,.vue .",
    "lint:all:stylelint": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "lint:all:prettier": "prettier --write --loglevel warn \"**/*.{js,json,css,scss,vue,html}\"",
    "lint": "run-s lint:all:*",
    "test:unit": "cross-env VUE_APP_TEST=unit vue-cli-service test:unit",
    "test:unit:file": "cross-env VUE_APP_TEST=unit vue-cli-service test:unit --bail --findRelatedTests",
    "test:unit:watch": "cross-env VUE_APP_TEST=unit vue-cli-service test:unit --watch --notify --notifyMode --passWithNoTests change",
    "test:unit:ci": "cross-env VUE_APP_TEST=unit vue-cli-service test:unit --coverage --ci",
    "test:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --headless",
    "test": "run-s test:unit test:e2e",
    "test:ci": "run-s test:unit:ci test:e2e"
  },
  "dependencies": {
    "axios": "0.19.0",
    "axios-cache-adapter": "2.3.3",
    "core-js": "3.1.4",
    "cross-env": "5.2.0",
    "localforage": "1.7.3",
    "localforage-memoryStorageDriver": "0.9.2",
    "lodash": "4.17.15",
    "normalize.css": "8.0.1",
    "npm": "6.10.3",
    "npm-run-all": "4.1.5",
    "nprogress": "0.2.0",
    "vue": "2.6.10",
    "vue-meta": "2.2.1",
    "vue-router": "3.1.1",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.0.0-beta.2",
    "@vue/cli-plugin-e2e-cypress": "4.0.0-beta.2",
    "@vue/cli-plugin-eslint": "4.0.0-beta.2",
    "@vue/cli-plugin-unit-jest": "4.0.0-beta.2",
    "@vue/cli-service": "4.0.0-beta.2",
    "@vue/eslint-config-prettier": "5.0.0",
    "@vue/eslint-config-standard": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "eslint": "6.1.0",
    "eslint-config-semistandard": "14.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-vue": "5.2.3",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "sass": "1.22.9",
    "sass-loader": "7.1.0",
    "stylelint": "10.1.0",
    "stylelint-config-css-modules": "1.4.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-recess-order": "2.0.3",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.9.3",
    "vue-template-compiler": "2.6.10"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  }
}
